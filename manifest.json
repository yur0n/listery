{
  "manifest_version": 3,
  "name": "ListeryApp",
  "description": "DESCR",
  "version": "1.0.0",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}