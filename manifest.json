{
  "manifest_version": 3,
  "name": "ListeryApp",
  "description": "DESCR",
  "version": "1.0.0",
  "permissions": [
    "storage"
  ],
  "action": {
    "default_icon": {
      		"16": "icons/16x16.png",     
      		"24": "icons/24x24.png",     
      		"32": "icons/32x32.png"      
    	},
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}